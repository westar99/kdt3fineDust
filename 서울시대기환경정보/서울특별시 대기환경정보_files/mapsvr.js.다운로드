map.removeLayer(tilelayer_tms_dawulgis_overlay_naver_hybrid)}catch(t){}},setOldMapOpacity=function(t){for(var i=0;i<tilelayer_tms_dawulgis_oldmap.length;i++)try{tilelayer_tms_dawulgis_oldmap[i].setOpacity(t)}catch(t){}},Coord_Trans=function(t,i){return initCS(t.toLowerCase()),cs_transform(csLeft,csRight,i),i};function e0fn(t){return 1-.25*t*(1+t/16*(3+1.25*t))}function e1fn(t){return.375*t*(1+.25*t*(1+.46875*t))}function e2fn(t){return.05859375*t*t*(1+.75*t)}function e3fn(t){return t*t*t*(35/3072)}function mlfn(t,i,e,o,s){return t*s-i*Math.sin(2*s)+e*Math.sin(4*s)-o*Math.sin(6*s)}function adjust_lon(t){return t=Math.abs(t)<PI?t:t-sign(t)*TWO_PI}function sign(t){return t<0?-1:1}function tmercInit(t){t.e0=e0fn(t.es),t.e1=e1fn(t.es),t.e2=e2fn(t.es),t.e3=e3fn(t.es),t.ml0=t.a*mlfn(t.e0,t.e1,t.e2,t.e3,t.lat0),t.ind=t.es<1e-5?1:0}function utmInit(t){t.lat0=0,t.long0=(6*Math.abs(t.zone)-183)*D2R,t.x0=5e5,t.y0=t.zone<0?1e7:0,t.k0||(t.k0=.9996),tmercInit(t)}function tmercFwd(t){var i,e,o,s=adjust_lon(t.x-this.long0),n=Math.sin(t.y),a=Math.cos(t.y);if(0!=this.ind){var r=a*Math.sin(s);if(Math.abs(Math.abs(r)-1)<1e-10)return 93;e=.5*this.a*this.k0*Math.log((1+r)/(1-r)),i=Math.acos(a*Math.cos(s)/Math.sqrt(1-r*r)),t.y<0&&(i=-i),o=this.a*this.k0*(i-this.lat0)}else{var h=a*s,l=Math.pow(h,2),c=this.ep2*Math.pow(a,2),m=Math.tan(t.y),u=Math.pow(m,2);i=1-this.es*Math.pow(n,2);var _=this.a/Math.sqrt(i),d=this.a*mlfn(this.e0,this.e1,this.e2,this.e3,t.y);e=this.k0*_*h*(1+l/6*(1-u+c+l/20*(5-18*u+Math.pow(u,2)+72*c-58*this.ep2)))+this.x0,o=this.k0*(d-this.ml0+_*m*(l*(.5+l/24*(5-u+9*c+4*Math.pow(c,2)+l/30*(61-58*u+Math.pow(u,2)+600*c-330*this.ep2)))))+this.y0}t.x=e,t.y=o}var utmFwd=tmercFwd;function tmercInv(t){var i,e,o,s,n,a;if(0!=this.ind){var r=exp(t.x/(this.a*this.k0)),h=.5*(r-1/r),l=this.lat0+t.y/(this.a*this.k0),c=cos(l);i=sqrt((1-c*c)/(1+h*h)),n=asinz(i),l<0&&(n=-n),a=0==h&&0==c?this.long0:adjust_lon(atan2(h,c)+this.long0)}else{for(t.x-=this.x0,t.y-=this.y0,e=i=(this.ml0+t.y/this.k0)/this.a,s=0;e+=o=(i+this.e1*Math.sin(2*e)-this.e2*Math.sin(4*e)+this.e3*Math.sin(6*e))/this.e0-e,!(Math.abs(o)<=EPSLN);s++)if(s>=6)return 95;if(Math.abs(e)<HALF_PI){var m=Math.sin(e),u=Math.cos(e),_=Math.tan(e),d=this.ep2*Math.pow(u,2),p=Math.pow(d,2),f=Math.pow(_,2),L=Math.pow(f,2);i=1-this.es*Math.pow(m,2);var g=this.a/Math.sqrt(i),y=g*(1-this.es)/i,P=t.x/(g*this.k0),v=Math.pow(P,2);n=e-g*_*v/y*(.5-v/24*(5+3*f+10*d-4*p-9*this.ep2-v/30*(61+90*f+298*d+45*L-252*this.ep2-3*p))),a=adjust_lon(this.long0+P*(1-v/6*(1+2*f+d-v/20*(5-2*d+28*f-3*p+8*this.ep2+24*L)))/u)}else n=HALF_PI*sign(t.y),a=this.long0}t.x=a,t.y=n}var utmInv=tmercInv,GEOCENT_LAT_ERROR=1,COS_67P5=.3826834323650898,AD_C=1.0026;function cs_geodetic_to_geocentric(t,i){var e,o,s,n,a,r,h,l=i.x,c=i.y,m=i.z,u=0;return c<-HALF_PI&&c>-1.001*HALF_PI?c=-HALF_PI:c>HALF_PI&&c<1.001*HALF_PI?c=HALF_PI:(c<-HALF_PI||c>HALF_PI)&&(u|=GEOCENT_LAT_ERROR),u||(l>PI&&(l-=2*PI),a=Math.sin(c),h=Math.cos(c),r=a*a,e=((n=t.a/Math.sqrt(1-t.es*r))+m)*h*Math.cos(l),o=(n+m)*h*Math.sin(l),s=(n*(1-t.es)+m)*a),i.x=e,i.y=o,i.z=s,u}function cs_geocentric_to_geodetic(t,i){var e,o,s,n,a,r,h,l,c,m,u,_,d,p,f,L,g,y=i.x,P=i.y,v=i.z;if(y=parseFloat(y),P=parseFloat(P),v=parseFloat(v),g=!1,0!=y)e=Math.atan2(P,y);else if(P>0)e=HALF_PI;else if(P<0)e=-HALF_PI;else if(g=!0,e=0,v>0)o=HALF_PI;else{if(!(v<0))return o=HALF_PI,void(s=-t.b);o=-HALF_PI}return a=y*y+P*P,n=Math.sqrt(a),r=v*AD_C,_=n/(l=Math.sqrt(r*r+a)),u=(m=r/l)*m*m,h=v+t.b*t.ep2*u,L=n-t.a*t.es*_*_*_,d=h/(c=Math.sqrt(h*h+L*L)),p=L/c,f=t.a/Math.sqrt(1-t.es*d*d),s=p>=COS_67P5?n/p-f:p<=-COS_67P5?n/-p-f:v/d+f*(t.es-1),0==g&&(o=Math.atan(d/p)),i.x=e,i.y=o,i.z=s,0}function cs_geocentric_to_wgs84(t,i){if(t.datum_type==PJD_3PARAM)i.x+=t.datum_params[0],i.y+=t.datum_params[1],i.z+=t.datum_params[2];else{var e=t.datum_params[0],o=t.datum_params[1],s=t.datum_params[2],n=t.datum_params[3],a=t.datum_params[4],r=t.datum_params[5],h=t.datum_params[6],l=h*(i.x-r*i.y+a*i.z)+e,c=h*(r*i.x+i.y-n*i.z)+o,m=h*(-a*i.x+n*i.y+i.z)+s;i.x=l,i.y=c,i.z=m}}function cs_geocentric_from_wgs84(t,i){if(t.datum_type==PJD_3PARAM)i.x-=t.datum_params[0],i.y-=t.datum_params[1],i.z-=t.datum_params[2];else{var e=t.datum_params[0],o=t.datum_params[1],s=t.datum_params[2],n=t.datum_params[3],a=t.datum_params[4],r=t.datum_params[5],h=t.datum_params[6],l=(i.x-e)/h,c=(i.y-o)/h,m=(i.z-s)/h;i.x=l+r*c-a*m,i.y=-r*l+c+n*m,i.z=a*l-n*c+m}}var PI=Math.PI,HALF_PI=.5*PI,TWO_PI=2*PI,R2D=57.2957795131,D2R=.0174532925199,EPSLN=1e-10,SRS_WGS84_SEMIMAJOR=6378137,SEC_TO_RAD=484813681109536e-20,PJD_UNKNOWN=0,PJD_3PARAM=1,PJD_7PARAM=2,PJD_GRIDSHIFT=3,PJD_WGS84=4,csErrorMessage="";function PT(t,i){this.x=t,this.y=i,this.z=0}var csList=new Object,csLeft,csRight;function CS(def){if(!def){var def=csList.EPSG4326;csErrorMessage+="No coordinate system definition provided, assuming longlat WGS83"}for(var paramName,paramVal,paramArray=def.split("+"),prop=0;prop<paramArray.length;prop++)switch(property=paramArray[prop].split("="),paramName=property[0].toLowerCase(),paramVal=property[1],paramName.replace(/\s/gi,"")){case"":break;case"title":this.title=paramVal;break;case"proj":this.proj=paramVal.replace(/\s/gi,"");break;case"a":this.a=parseFloat(paramVal);break;case"b":this.b=parseFloat(paramVal);break;case"lon_0":this.long0=paramVal*D2R;break;case"lat_0":this.lat0=paramVal*D2R;break;case"x_0":this.x0=parseFloat(paramVal);break;case"y_0":this.y0=parseFloat(paramVal);break;case"k":this.k0=parseFloat(paramVal);break;case"to_meter":this.to_meter=eval(paramVal);break;case"zone":this.zone=parseInt(paramVal);break;case"towgs84":this.datum_params=paramVal.split(",");break;case"from_greenwich":this.from_greenwich=paramVal*D2R;break;default:csErrorMessage+="\nUnrecognized parameter: "+paramName}if(this.datum_params){for(var i=0;i<this.datum_params.length;i++)this.datum_params[i]=parseFloat(this.datum_params[i]);0==this.datum_params[0]&&0==this.datum_params[1]&&0==this.datum_params[2]||(this.datum_type=PJD_3PARAM),this.datum_params.length>3&&(0==this.datum_params[3]&&0==this.datum_params[4]&&0==this.datum_params[5]&&0==this.datum_params[6]||(this.datum_type=PJD_7PARAM,this.datum_params[3]*=SEC_TO_RAD,this.datum_params[4]*=SEC_TO_RAD,this.datum_params[5]*=SEC_TO_RAD,this.datum_params[6]=this.datum_params[6]/1e6+1))}this.datum_type||(this.datum_type=PJD_WGS84),this.a||(this.a=6378137,this.b=6356752.31424518,csErrorMessage+="\nEllipsoid parameters not provided, assuming WGS84"),this.a2=this.a*this.a,this.b2=this.b*this.b,this.es=(this.a2-this.b2)/this.a2,this.e=Math.sqrt(this.es),this.ep2=(this.a2-this.b2)/this.b2,"longlat"!=this.proj&&(this.Forward=eval(this.proj+"Fwd"),this.Inverse=eval(this.proj+"Inv"),this.Init=eval(this.proj+"Init"),this.Init(this))}function cs_transform(t,i,e){if(pj_errno=0,"longlat"==t.proj?(e.x*=D2R,e.y*=D2R):(t.to_meter&&(e.x*=t.to_meter,e.y*=t.to_meter),t.Inverse(e)),t.from_greenwich&&(e.x+=t.from_greenwich),0!=cs_datum_transform(t,i,e))return pj_errno;i.from_greenwich&&(e.x-=i.from_greenwich),"longlat"==i.proj?(e.x*=R2D,e.y*=R2D):(i.Forward(e),i.to_meter&&(e.x/=i.to_meter,e.y/=i.to_meter))}function cs_datum_transform(t,i,e){return cs_compare_datums(t,i)?0:(t.datum_type==PJD_GRIDSHIFT&&alert("ERROR: Grid shift transformations are not implemented yet."),i.datum_type==PJD_GRIDSHIFT&&alert("ERROR: Grid shift transformations are not implemented yet."),t.datum_type!=PJD_3PARAM&&t.datum_type!=PJD_7PARAM&&i.datum_type!=PJD_3PARAM&&i.datum_type!=PJD_7PARAM||(cs_geodetic_to_geocentric(t,e),t.datum_type!=PJD_3PARAM&&t.datum_type!=PJD_7PARAM||cs_geocentric_to_wgs84(t,e),i.datum_type!=PJD_3PARAM&&i.datum_type!=PJD_7PARAM||cs_geocentric_from_wgs84(i,e),cs_geocentric_to_geodetic(i,e)),i.datum_type==PJD_GRIDSHIFT&&alert("ERROR: Grid shift transformations are not implemented yet."),0)}function cs_compare_datums(t,i){return t.datum_type!=i.datum_type?0:t.datum_type==PJD_3PARAM?t.datum_params[0]==i.datum_params[0]&&t.datum_params[1]==i.datum_params[1]&&t.datum_params[2]==i.datum_params[2]:t.datum_typ